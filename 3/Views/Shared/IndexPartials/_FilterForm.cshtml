@using _3.Models

@{
    SearchModel searchModel = ViewBag.searchModel;
}

@using (Html.BeginForm())
{
    <p>
        <div class="row">
            <div class="col-md-12">
                @Html.EditorFor(model => searchModel, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <input type="submit" class="mt-3 btn btn-outline-dark btn-secondary" value="Search" />
            </div>
        </div>
</p>
}